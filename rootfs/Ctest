// Simple C test program for Lakos OS
// This would be compiled to a binary that accepts arguments

#include <stdio.h>
#include <stdlib.h>

int main(int argc, char* argv[]) {
    printf("Test binary executed with %d arguments:\n", argc - 1);
    for (int i = 1; i < argc; i++) {
        printf("Arg %d: %s\n", i, argv[i]);
    }
    return 0;
}